{
  "manifest_version": 2,

  "name": "Time Limit by URL",
  "description": "Set time limit for browsing of specific URL",
  "version": "1.0",

  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "permissions": [
    "activeTab",
    "https://ajax.googleapis.com/"
  ]
}
